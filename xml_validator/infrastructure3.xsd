<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="https://www.railml.org/schemas/3.1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:rail3="https://www.railml.org/schemas/3.1" xmlns:gml4rail3="https://www.railml.org/schemas/3.1/gml" elementFormDefault="qualified" version="3.1">
